{ "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
],
"parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
},
"env": {
    "browser": true,
    "node": true, 
    "jasmine": true
},
"rules": {
    "react/prop-types": "off"
  }
}  